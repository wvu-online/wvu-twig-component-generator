{
  "name": "generator-wvu-twig-component",
  "version": "0.0.1",
  "description":
    "Generate files for a twig component on WVU Online Drupal themes",
  "homepage": "https://github.com/wvu-online/wvu-twig-component-generator",
  "author": {
    "name": "Gregg Shanefelt-WVU Online",
    "email": "gregg.shanefelt@mail.wvu.edu",
    "url": ""
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": ["drupal", "twig", "emulsify", "patternlab", "yeoman-generator"],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^6.1.1",
    "nsp": "^2.8.0",
    "prettier": "^1.11.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "generator-wvu-twig-component": "^0.0.1",
    "global": "^4.3.2",
    "lowdash": "^1.2.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "wvu-online/generator-wvu-twig-component",
  "license": "GPL-3.0"
}
